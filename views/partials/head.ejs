<title> inBDPA: <%= title %> </title>
<script>
    document.addEventListener('DOMContentLoaded', function() {
            fetch('/getelectioninfo')
                .then(response => response.json())
                .then(data => {
                    document.getElementById('upcomingElections').innerText = data.upcomingElections;
                    document.getElementById('openElections').innerText = data.openElections;
                    document.getElementById('closedElections').innerText = data.closedElections;
                });
    });
</script>


<meta name="viewport" content="width=device-width, initial-scale=1">
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>

<link rel='stylesheet' href='/stylesheets/style.css' />
